{
  "name": "redis-hsetex",
  "version": "3.0.0",
  "description": "hsetex lua command for redis clients",
  "main": "dist/index.cjs",
  "type": "module",
  "engines": {
    "node": ">=12.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha test/unit",
    "test:integration": "mocha test/integration",
    "build": "gulp -f gulpfile.cjs build",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotcore64/redis-hsetex.git"
  },
  "keywords": [
    "redis"
  ],
  "author": "Julian Grinblat <julian@dotcore.co.il>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotcore64/redis-hsetex/issues"
  },
  "homepage": "https://github.com/dotcore64/redis-hsetex#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dirname-filename-esm": "^1.1.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-mocha": "^10.0.3",
    "gulp": "^4.0.2",
    "gulp-redis-lua2js": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "ioredis": "^4.28.3",
    "mocha": "^9.1.4",
    "redis-pdel": "^3.0.0"
  }
}
